{
  "name": "n1pluginupdater",
  "version": "0.5.0",
  "description": "In lieu of official support, this module displays a banner directing users to update",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/eslint/bin/eslint.js . --ext .js,.jsx,.es6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KyleKing/n1pluginupdater"
  },
  "author": "Kyle King",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^0.30.0",
    "github": "^0.2.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "NylasEnv": true
    },
    "rules": {
      "react/prop-types": [
       2,
        {
          "ignore": [
            "children"
          ]
        }
      ],
      "react/no-multi-comp": [0],
      "react/sort-comp": [2],
      "react/jsx-no-bind": [2, { "allowBind": true }],
      "no-alert": [0],
      "eqeqeq": [2, "smart" ],
      "id-length": [0],
      "object-curly-spacing": [0],
      "no-console": [0],
      "no-loop-func": [0],
      "no-constant-condition": [0],
      "new-cap": [2, { "capIsNew": false }],
      "no-shadow": [1],
      "quotes": [0],
      "semi": [0],
      "import/no-unresolved": [0],
      "no-param-reassign": [0],
      "no-underscore-dangle": ["error", { "allowAfterThis": true }]
    },
    "parser": "babel-eslint"
  }
}
